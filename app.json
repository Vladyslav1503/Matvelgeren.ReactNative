{
  "expo": {
    "name": "Matvelgeren",
    "slug": "Matvelgeren",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_white.png",
    "scheme": "matvelgeren",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "userInterfaceStyle": "light",
      "supportsTablet": false,
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": false
      }
    },
    "android": {
      "adaptiveIcon": {},
      "package": "com.anonymous.Matvelgeren"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_white.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "android": {
        "navigationBar": {
          "visible": "leanback",
          "backgroundColor": "#ffffff",
          "barStyle": "dark-content"
        },
        "softwareKeyboardLayoutMode": "pan",
        "statusBar": {
          "backgroundColor": "#ffffff",
          "barStyle": "dark-content",
          "translucent": false
        }
      },
      "router": {},
      "eas": {
        "projectId": "dc162795-f505-4760-b087-6dfbf56f07ae"
      }
    }
  }
}
